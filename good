nvcc -arch=sm_30 -Xcompiler -m64 -DDOUBLE -DDEBUG -g -G  -dc shigan_kernel.cu -I /home/asiera/NVIDIA_CUDA-5.0_Samples/common/inc -I /usr/local/cuda/lib64/include -I /home/asiera/NVIDIA_CUDA-5.0_Samples/0_Simple/simplePrintf/ -I /usr/lib/openmpi/include -I /usr/local/cgns/include
nvcc -arch=sm_30 -Xcompiler -m64 -DDOUBLE -DDEBUG -g -G  -dlink cuda_bluebottle.o cuda_bicgstab.o cuda_point.o cuda_testing.o entrySearch.o bluebottle_kernel.o bicgstab_kernel.o entrySearch_kernel.o point_kernel.o shigan.o shigan_kernel.o -o bblib.o -L /home/asiera/NVIDIA_CUDA-5.0_Samples/lib -L /usr/local/cuda/lib64 -lcudart
mpicc.openmpi -std=c99 -pedantic -Wall -Wextra -fopenmp -DDOUBLE -DDEBUG -g  -c bluebottle.c -I /usr/lib/openmpi/include -I /usr/local/cgns/include
mpicc.openmpi -std=c99 -pedantic -Wall -Wextra -fopenmp -DDOUBLE -DDEBUG -g  -c domain.c -I /usr/lib/openmpi/include -I /usr/local/cgns/include
mpicc.openmpi -std=c99 -pedantic -Wall -Wextra -fopenmp -DDOUBLE -DDEBUG -g  -o bluebottle cuda_bluebottle.o cuda_bicgstab.o cuda_point.o cuda_testing.o entrySearch.o bluebottle_kernel.o bicgstab_kernel.o entrySearch_kernel.o point_kernel.o shigan.o shigan_kernel.o bblib.o bluebottle.o domain.o point.o precursor.o recorder.o seeder.o vtk.o -lm -L /usr/local/hdf5/lib -L /usr/local/cgns/lib -lcgns -lhdf5  -L /home/asiera/NVIDIA_CUDA-5.0_Samples/lib -L /usr/local/cuda/lib64 -lcudart -lstdc++
